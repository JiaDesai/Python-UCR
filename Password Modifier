word = list(input())
password = ''

for char in word: 
    if char == 'i': 
        char = '1'
    if char == 'a':
        char = '@'
    if char == 'm': 
        char = 'M'
    if char == 'B': 
        char = '8'
    if char == 's': 
        char = '$'
    password += char

password = password + '!'
print(password)
